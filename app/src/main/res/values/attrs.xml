<resources>
  <declare-styleable name="TitleBar">
    <attr format="reference" name="src"/>
    <attr format="reference" name="rightSrc"/>
  </declare-styleable>
  <declare-styleable name="MaterialProgressBar">
    <attr format="color" name="progressColour"/>
    <attr format="color" name="secondaryProgressColour"/>
    <attr format="color" name="backgroundColour"/>
    <attr format="integer" name="duration"/>
  </declare-styleable>
  <declare-styleable name="TypefacedTextView">
    <attr format="enum" name="fontName">
      <enum name="avenirMedium" value="0"/>
      <enum name="avenirBlack" value="1"/>
      <enum name="avenirRoman" value="2"/>
      <enum name="avenirHeavy" value="3"/>
      <enum name="avenirLight" value="4"/>
    </attr>
  </declare-styleable>
  <declare-styleable name="TypefacedEditText">
    <attr format="enum" name="fontEditText">
      <enum name="avenirMedium" value="0"/>
      <enum name="avenirBlack" value="1"/>
      <enum name="avenirRoman" value="2"/>
      <enum name="avenirHeavy" value="3"/>
      <enum name="avenirLight" value="4"/>
    </attr>
  </declare-styleable>
  <declare-styleable name="TypefacedButton">
    <attr format="enum" name="fontButton">
      <enum name="avenirMedium" value="0"/>
      <enum name="avenirBlack" value="1"/>
      <enum name="avenirRoman" value="2"/>
      <enum name="avenirHeavy" value="3"/>
      <enum name="avenirLight" value="4"/>
    </attr>
  </declare-styleable>
</resources>